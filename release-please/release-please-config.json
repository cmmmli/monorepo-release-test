{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",

  "bootstrap-sha": "5acf3056d9416626903bc40fcdc90848ec4610bf",

  "release-type": "node",
  "separate-pull-requests": false,
  "include-component-in-tag": true,
  "bump-minor-pre-major": true,
  "changelog-sections": [
    { "type": "feat", "section": "Features", "hidden": false },
    { "type": "fix", "section": "Bug Fixes", "hidden": false },
    { "type": "chore", "section": "Miscellaneous", "hidden": false }
  ],
  "packages": {
    "apps/docs": { "component": "docs" },
    "apps/web": { "component": "web" },
    "apps/server": { "component": "server", "release-type": "simple" },
    "packages/hello": { "component": "hello", "skip-github-release": true }
  },
  "plugins": [
    {
      "type": "linked-versions",
      "groupName": "web",
      "components": [
        "web", "server", "hello"
      ]
    },
    {
      "type": "linked-versions",
      "groupName": "docs",
      "components": [
        "docs"
      ]
    }
  ]
}
